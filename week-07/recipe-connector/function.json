{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "twilioSms",
      "direction": "out",
      "name": "message",
      "accountSidSetting": "TwilioAccountSid",
      "authTokenSetting": "TwilioAuthToken"
    }
  ]
}
